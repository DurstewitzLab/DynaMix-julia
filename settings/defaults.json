{   
    "experiment": "default",
    "name": "default",
    "path_to_data": "example_data/training_data_3D.npy",
    "path_to_context": "example_data/training_context_3D.npy",
    "path_to_test_data": "example_data/Test_DS.npy",
    "run": 1,

    "model": "DynaMix",
    "observation_model": "Identity",
    "latent_dim": 30,
    "pwl_units": 2,
    "experts": 10,

    "lat_model_regularization": 0.1,
    "obs_model_regularization": 0.0,

    "optimizer": "RAdam",
    "gradient_clipping_norm": 10.0,
    "start_lr": 5e-3,
    "end_lr": 1e-5, 
    "batch_size": 16,
    "sequence_length": 98,
    "teacher_forcing_interval": 10,
    "use_gtf": false,
    "gtf_alpha": 0.1,
    "gtf_alpha_method": "constant",
    "gtf_alpha_decay": 0.999,
    "alpha_update_interval": 5,
    "partial_forcing": true,

    "epochs": 2000,
    "batches_per_epoch": 50,
    "scalar_saving_interval": 25,
    "image_saving_interval": 25,
    "gaussian_noise_level": 0.05,

    "D_stsp_bins": 30,
    "D_stsp_scaling": 1.0,
    "PSE_smoothing": 20.0,
    "PE_n": 10
}
